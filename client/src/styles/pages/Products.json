{
  "pageContainer": {
    "display": "flex",
    "flexDirection": "column",
    "minHeight": "100vh",
    "maxWidth": "800px",
    "margin": "auto",
    "position": "relative"
  },
  "backgroundCard": {
    "backgroundColor": "#FFFFFF",
    "width": "100%"
  },
  "productsCard": {
    "backgroundColor": "#F0D794",
    "width": "100%",
    "height": "100%",
    "display": "flex",
    "justifyContent": "center"
  },
  "weekPickCopy": {
    "fontSize": 15,
    "fontWeight": "bold",
    "letterSpacing": 1
  },
  "avatarContainer": {
    "display": "flex",
    "margin": "8em 2em 0"
  },
  "avatar": {
    "top": 90,
    "left": 20
  },
  "speechBubble": {
    "top": 95,
    "left": 160
  },
  "profile": {
    "position": "absolute",
    "top": 15,
    "left": 10
  },
  "usernameCopy": {
    "fontSize": 12,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "marginTop": 5
  },
  "panel": {
    "display": "flex",
    "justifyContent": "center",
    "flexGrow": 1
  },
  "divider": {
    "width": "100%"
  },
  "header": {
    "width": "100%",
    "display": "flex",
    "justifyContent": "center",
    "marginBottom": "1em"
  },
  "cartDialog": {
    "display": "flex",
    "justifyContent": "center"
  }
}
