{
  "container": { 
    "height": 500, 
    "width": "100%", 
    "backgroundColor": "#F0D794",
    "display": "flex",
    "justifyContent": "center"
  },
  "innerCard": {
    "width": "80%",
    "backgroundColor": "#FFFFFF",
    "height": "85%",
    "marginTop": 20,
    "borderRadius": "2em",
    "border": "2px solid black"
  },
  "listContainer": {
    "display": "flex",
    "flexDirection": "column",
    "gap": "0.7em",
    "height": "30em",
    "overflow": "auto",
    "scrollbarWidth": "none",
    "padding": "1em"
  },
  "totalLine": {
    "display": "flex",
    "justifyContent": "right",
    "margin": "0.8em 0.8em auto auto",
    "fontFamily": "Inter",
    "fontSize": "1.8rem",
    "fontWeight": 700
  },
  "textCopy": {
    "marginTop": "0.8em",
    "fontSize": "1.6rem",
    "fontWeight": 500,
    "fontFamily": "Inter",
    "marginLeft": "0.8em"
  }
}
