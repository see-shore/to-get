{
  "container": {
    "minHeight": "100%",
    "width": "100%",
    "backgroundColor": "#F0D794",
    "display": "flex",
    "justifyContent": "center",
    "paddingBottom": "7em"
  },
  "innerCard": {
    "width": "80%",
    "backgroundColor": "#FFFFFF",
    "height": "100%",
    "marginTop": 20,
    "borderRadius": "2em",
    "border": "2px solid black",
    "position": "relative",
    "paddingBottom": "10.5em"
  },
  "listContainer": {
    "display": "flex",
    "flexDirection": "column",
    "gap": "0.7em",
    "overflow": "auto",
    "scrollbarWidth": "none",
    "padding": "0 1em 1em",
    "maxHeight": "95%",
    "minHeight": "100px",
    "marginTop": "1em"
  },
  "totalLine": {
    "display": "flex",
    "justifyContent": "right",
    "margin": "0.8em 0.8em auto auto",
    "fontFamily": "Inter",
    "fontSize": "1.8rem",
    "fontWeight": 700
  },
  "textCopy": {
    "marginTop": "0.8em",
    "fontSize": "1.6rem",
    "fontWeight": 500,
    "fontFamily": "Inter",
    "marginLeft": "0.8em"
  },
  "textContainer": {
    "position": "absolute",
    "inset": "auto 1em 2em"
  }
}
