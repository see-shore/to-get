  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "minHeight": 250
  },
  "cardCopy": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold"
  },
  "productPicture": {
    "width": 125,
    "height": 125,
    "backgroundColor": "#C4C4C4",
    "borderRadius": 10,
    "marginRight": 20
