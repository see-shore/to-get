{
  "container": {
    "backgroundColor": "#9C9C9C",
    "width": 160,
    "height": 200,
    "margin": "10px 10px 10px 10px",
    "borderRadius": 5,
    "display": "flex",
    "justifyContent": "center"
  },
  "productName": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "display": "flex",
    "justifyContent": "center",
    "width": 160,
    "marginTop": 10
  },
  "counter": {
    "height": 30,
    "display": "flex",
    "justifyContent": "space-between"
  },
  "numberDisplay": {
    "backgroundColor": "#FFFFFF",
    "width": 40,
    "height": 25,
    "borderRadius": 5,
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "price": {
    "backgroundColor": "#FFFFFF",
    "width": 100,
    "height": 25,
    "borderRadius": 2,
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center",
    "marginLeft": 6,
    "marginTop": 2
