{
  "container": {
    "width": 140,
    "minWidth": 140,
    "maxWidth": "45vw",
    "minHeight": 170,
    "position": "relative",
    "margin": "1em 0.5em"
  },
  "counter": {
    "height": 30,
    "display": "flex",
    "justifyContent": "space-between"
  },
  "numberDisplay": {
    "backgroundColor": "#FFFFFF",
    "width": 40,
    "height": 25,
    "borderRadius": 5,
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "price": {
    "backgroundColor": "#FFFFFF",
    "width": 100,
    "height": 25,
    "borderRadius": 2,
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center",
    "marginLeft": 6,
    "marginTop": 2
  },
  "priceUnit": {
    "marginLeft": 2
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "minHeight": 250
  },
  "cardCopy": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold"
  },
  "productPicture": {
    "width": 125,
    "height": 125,
    "backgroundColor": "#C4C4C4",
    "borderRadius": 10,
    "marginRight": 20
  },
  "description": {
    "width": 125,
    "height": 60,
    "backgroundColor": "#F1F1F1",
    "borderRadius": 5,
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "itemName": {
    "fontSize": "1.5rem",
    "fontWeight": 600,
    "textAlign": "center",
    "paddingTop": 5
  },
  "dialogPrice": {
    "fontSize": 17,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "marginTop": 10
  },
  "iconButton": {
    "backgroundColor": "#C4C4C4"
  },
  "box": {
    "height": "100%",
    "display": "flex",
    "flexDirection": "column",
    "cursor": "pointer"
  },
  "crate": {
    "background": "rgb(210, 161, 120)",
    "flexGrow": 1,
    "borderRadius": "20px 0 20px 20px",
    "border": "3px solid black",
    "paddingBottom": 50
  },
  "priceTag": {
    "background": "#ffffffd4",
    "minWidth": 75,
    "maxWidth": 124,
    "padding": "0 10px",
    "margin": "0 0 0 auto",
    "height": 26,
    "display": "flex",
    "alignItems": "center",
    "borderRadius": "10px 10px 0 0",
    "border": "3px solid black",
    "borderBottom": "none",
    "justifyContent": "center",
    "overflow": "hidden"
  },
  "priceText": {
    "fontSize": "1.2rem",
    "fontWeight": 700
  },
  "control": {
    "display": "flex",
    "position": "absolute",
    "alignItems": "center",
    "inset": "auto 0 12px 0",
    "justifyContent": "center",
    "fontSize": "1.4rem",
    "border": "3px solid #6a5442",
    "width": "fit-content",
    "margin": "auto",
    "borderRadius": 10
  },
  "button": {
    "padding": "0.2em 0.5em",
    "color": "#6a5442",
    "borderRadius": 0
  },
  "countText": {
    "width": 35,
    "textAlign": "center"
  },
  "imageContainer": {
    "height": 70,
    "width": "calc(100% + 6px)",
    "borderRadius": 20,
    "borderTopRightRadius": 0,
    "border": "3px solid black",
    "margin": -3,
    "objectFit": "cover"
  },
  "itemType": {
    "textAlign": "center",
    "fontSize": "1.2rem"
  },
  "textContainer": {
    "minHeight": 40,
    "alignItems": "center",
    "display": "flex",
    "justifyContent": "center",
    "flexDirection": "column"
  }
}
