  "description": {
    "width": 125,
    "height": 60,
    "backgroundColor": "#F1F1F1",
    "borderRadius": 5,
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "itemName": {
    "fontSize": 20,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "marginBottom": 10
  },
  "dialogPrice": {
    "fontSize": 17,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "marginTop": 10
