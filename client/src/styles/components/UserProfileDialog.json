{
  "avatar": {
    "width": 40,
    "height": 40,
    "borderRadius": "50%",
    "border": "3px solid black",
    "zIndex": 100
  },
  "user": {
    "cursor": "pointer",
    "display": "flex",
    "alignItems": "center"
  },
  "userName": {
    "border": "3px solid black",
    "height": 40,
    "margin": -20,
    "paddingLeft": 30,
    "paddingRight": 20,
    "borderRadius": "0 2em 2em 0",
    "fontWeight": 700,
    "fontSize": "1.4rem",
    "letterSpacing": "0.15em",
    "backgroundColor": "#D9D9D9",
    "maxWidth": 250,
    "overflow": "hidden",
    "textOverflow": "ellipsis",
    "whiteSpace": "nowrap",
    "lineHeight": "33px"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "minHeight": 250,
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "text": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold"
  }
}
