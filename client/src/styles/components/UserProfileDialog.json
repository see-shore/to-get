{
  "avatar": {
    "width": 40,
    "height": 40,
    "borderRadius": "50%",
    "border": "3px solid black",
    "zIndex": 100,
    "backgroundColor": "white",
    "display": "flex",
    "alignItems": "center",
    "textAlign": "center"
  },
  "user": {
    "cursor": "pointer",
    "display": "flex",
    "alignItems": "center"
  },
  "userName": {
    "border": "3px solid black",
    "height": 40,
    "margin": -20,
    "paddingLeft": 30,
    "paddingRight": 20,
    "borderRadius": "0 2em 2em 0",
    "fontWeight": 700,
    "fontSize": "1.4rem",
    "letterSpacing": "0.15em",
    "backgroundColor": "#D9D9D9",
    "maxWidth": 250,
    "overflow": "hidden",
    "textOverflow": "ellipsis",
    "whiteSpace": "nowrap",
    "lineHeight": "33px"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "display": "flex",
    "justifyContent": "center"
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "header": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "marginTop": 30
  },
  "text": {
    "fontSize": 13,
    "fontFamily": "helvetica",
    "marginTop": 5
  },
  "leftPanel": {
    "width": "35%"
  },
  "rightPanel": {
    "width": "65%",
    "margin": "20px auto auto 10px"
  },
  "button": {
    "color": "#FFFFFF",
    "backgroundColor": "#000000",
    "width": 110,
    "borderRadius": 10,
    "height": 35,
    "marginRight": 1,
    "marginBottom": 1,
    "fontSize": 10,
    "fontFamily": "helvetica",
    "fontWeight": "bold"
  },
  "image": {
    "marginTop": 30,
    "marginRight": 10,
    "maxWidth": 80,
    "borderRadius": "50%",
    "border": "5px solid black",
    "zIndex": 100
  },
  "dialog": {}
}
