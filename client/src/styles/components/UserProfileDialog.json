{
  "avatar": {
    "width": 40,
    "height": 40,
    "borderRadius": "50%",
    "border": "3px solid black",
    "zIndex": 100
  },
  "user": {
    "cursor": "pointer",
    "display": "flex",
    "alignItems": "center"
  },
  "userName": {
    "border": "3px solid black",
    "height": 40,
    "alignItems": "center",
    "display": "flex",
    "margin": -20,
    "paddingLeft": "calc(20px + 1em)",
    "paddingRight": "2em",
    "borderRadius": "0 2em 2em 0",
    "fontWeight": 700,
    "fontSize": "1.2rem",
    "letterSpacing": 1,
    "backgroundColor": "#D9D9D9"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "minHeight": 250,
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center"
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "text": {
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold"
  }
}
