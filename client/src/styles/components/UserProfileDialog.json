{
  "avatar": {
    "width": 40,
    "height": 40,
    "borderRadius": "50%",
    "border": "3px solid black",
    "zIndex": 100,
    "backgroundColor": "white",
    "display": "flex",
    "alignItems": "center",
    "textAlign": "center"
  },
  "user": {
    "cursor": "pointer",
    "display": "flex",
    "alignItems": "center",
    "zIndex": 100,
    "maxWidth": "37vw"
  },
  "userName": {
    "border": "3px solid black",
    "height": 40,
    "margin": -20,
    "paddingLeft": 30,
    "paddingRight": 20,
    "borderRadius": "0 2em 2em 0",
    "fontWeight": 700,
    "fontSize": "1.4rem",
    "letterSpacing": "0.15em",
    "maxWidth": 250,
    "overflow": "hidden",
    "textOverflow": "ellipsis",
    "whiteSpace": "nowrap",
    "lineHeight": "33px"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContent": {
    "display": "flex",
    "flexDirection": "column",
    "justifyContent": "center",
    "marginTop": "1em",
    "paddingBottom": "1em"
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "header": {
    "fontSize": "1.6rem",
    "fontWeight": "bold",
    "overflow": "hidden",
    "textOverflow": "ellipsis"
  },
  "text": {
    "fontSize": "1.4rem",
    "marginTop": 5,
    "overflow": "hidden",
    "textOverflow": "ellipsis"
  },
  "topPanel": {
    "width": "100%"
  },
  "bottomPanel": {
    "display": "flex",
    "flexDirection": "column",
    "justifyContent": "center",
    "textAlign": "center",
    "margin": "auto",
    "maxWidth": "100%"
  },
  "button": {
    "color": "#FFFFFF",
    "display": "flex",
    "justifyContent": "center",
    "border": "none",
    "borderRadius": "5em",
    "width": "15em",
    "maxWidth": 400,
    "gap": "0.5em",
    "marginBottom": "1em",
    "letterSpacing": 1
  },
  "buttonText": {
    "fontSize": "1.4rem",
    "fontWeight": 500
  },
  "imageContainer": {
    "height": 80,
    "width": 80,
    "borderRadius": "50%",
    "overflow": "hidden",
    "border": "3px solid black",
    "zIndex": 100,
    "marginTop": 30,
    "textAlign": "center",
    "margin": "auto",
    "marginBottom": "1.5em"
  },
  "image": {
    "height": "100%",
    "width": "100%"
  },
  "dialog": {},
  "footer": {
    "margin": "auto"
  }
}
