{
  "row": {
    "cursor": "pointer"
  },
  "dialogContent": {
    "maxHeight": 800,
    "maxWidth": 800,
    "display": "flex",
    "justifyContent": "center"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContainer": {
    "display": "flex",
    "justifyContent": "center",
    "minWidth": 150
  },
  "textField": {
    "marginTop": 2
  },
  "availableRow": {
    "display": "flex",
    "justifyContent": "space-between",
    "alignItems": "center",
    "margin": "10px auto auto auto",
    "backgroundColor": "#609966",
    "borderRadius": 2,
    "opacity": 0.7,
    "color": "#FFFFFF"
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "itemImage": {
    "maxWidth": 300,
    "maxHeight": 300,
    "margin": "20px 20px 10px auto"
  },
  "instruction": {
    "fontWeight": "bold",
    "fontFamily": "helvetica",
    "marginBottom": 5,
    "fontSize": 12
  },
  "fileInput": {
    "maxWidth": 300
  }
}
