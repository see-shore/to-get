{
  "row": {
    "cursor": "pointer"
  },
  "dialogContent": {
    "maxHeight": 800,
    "maxWidth": 800,
    "display": "flex",
    "justifyContent": "center"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialogContainer": {
    "display": "flex",
    "justifyContent": "center",
    "minWidth": 150
  },
  "textField": {
    "marginTop": 2
  },
  "icon": {
    "margin": "auto 5px auto 5px"
  },
  "itemImage": {
    "maxWidth": 300,
    "maxHeight": 300,
    "margin": "20px 20px 10px auto"
  },
  "instruction": {
    "fontWeight": "bold",
    "fontFamily": "helvetica",
    "marginBottom": 5,
    "fontSize": 12
  },
  "fileInput": {
    "maxWidth": 300
  },
  "availableButton": {
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center",
    "backgroundColor": "#609966",
    "color": "#FFFFFF",
    "cursor": "pointer",
    "borderRadius": 5,
    "opacity": 0.8,
    "width": "100%",
    "height": 25,
    "marginTop": 20
  },
  "unavailableButton": {
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center",
    "backgroundColor": "#eb5350",
    "color": "#FFFFFF",
    "cursor": "pointer",
    "borderRadius": 5,
    "width": "100%",
    "height": 25,
    "marginTop": 20
  },
  "buttonCopy": {
    "fontFamily": "helvetica",
    "fontSize": 12
  },
  "yesCopy": {
    "fontWeight": "bold",
    "color": "#609966"
  },
  "noCopy": {
    "fontWeight": "bold",
    "color": "#eb5350"
  }
}
