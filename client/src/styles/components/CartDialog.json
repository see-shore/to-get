{
  "button": {
    "width": "90%",
    "position": "fixed",
    "bottom": 10,
    "height": 40,
    "backgroundColor": "#000000",
    "color": "#FFFFFF",
    "display": "flex",
    "justifyContent": "center",
    "alignItems": "center",
    "fontSize": 15,
    "fontFamily": "helvetica",
    "fontWeight": "bold",
    "cursor": "pointer",
    "border": "none",
    "borderRadius": 20,
    "padding": "0 2em",
    "maxWidth": 900,
    "minWidth": "fit-content"
  },
  "icon": {
    "width": 30,
    "height": 30,
    "marginRight": 1,
    "color": "#FFFFFF",
    "display": "flex",
    "alignItems": "center"
  },
  "itemCount": {
    "marginLeft": "0.5em"
  },
  "dialogTitle": {
    "display": "flex",
    "justifyContent": "space-between"
  },
  "dialog": {
    "width": "100%",
    "maxWidth": "100vw"
  },
  "dialogContent": {
    "minHeight": 250,
    "padding": "20px 12px"
  },
  "emptyDalogContent": {
    "minHeight": 150
  },
  "cartCopy": {
    "fontSize": 15,
    "fontWeight": "bold"
  },
  "totalLine": {
    "display": "flex",
    "justifyContent": "right",
    "marginTop": "0.8em",
    "fontFamily": "Inter",
    "fontSize": "1.8rem",
    "fontWeight": 700
  },
  "textCopy": {
    "marginTop": "0.8em",
    "fontSize": "1.6rem",
    "fontWeight": 500,
    "fontFamily": "Inter"
  },
  "orderButton": {
    "display": "flex",
    "justifyContent": "center",
    "marginTop": 20
  },
  "itemContainer": {
    "display": "flex",
    "flexDirection": "column",
    "gap": "0.7em",
    "maxHeight": "27em",
    "overflow": "auto",
    "scrollbarWidth": "none"
  },
  "paperStyle": {
    "style": {
      "borderRadius": "2em",
      "border": "2px solid black",
      "width": "100%",
      "margin": "24px",
      "maxHeight": "65vh"
    }
  }
}
