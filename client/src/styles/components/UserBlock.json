{
  "block": {
    "color": "#FFFFFF",
    "backgroundColor": "#609966",
    "borderRadius": 2,
    "opacity": 0.7,
    "width": 300,
    "height": 50,
    "margin": "auto auto 20px 50px",
    "cursor": "pointer"
  },
  "avatar": {
    "width": 40,
    "height": 40,
    "borderRadius": "50%",
    "marginTop": 5,
    "marginLeft": 5
  },
  "userDetails": {
    "display": "flex",
    "justifyContent": "start",
    "alignItems": "center"
  },
  "userText": {
    "margin": "10px auto auto 15px"
  }
}
